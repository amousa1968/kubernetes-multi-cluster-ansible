---
# Ansible Lint configuration
parseable: true
quiet: true
skip_list:
  - 'schema'
  - 'no-handler'
  - 'fqcn-builtins'
  - 'fqcn[action-core]'
  - 'name[missing]'
  - 'name[template]'
  - 'name[play]'
exclude_paths:
  - roles/geerlingguy.docker/
  - roles/geerlingguy.kubernetes/
  - roles/cloudalchemy.node_exporter/
use_default_rules: true
